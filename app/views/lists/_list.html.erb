<tr id="list_<%= list.id %>">
	<td class="name"><%= link_to list.name, list, remote: true %></td>
	<td><%= link_to '', edit_list_path(list), remote: true, :class=>"icon-edit"%></td>
	<td><%= link_to '', list, method: :delete, data: { confirm: 'Are you sure?' }, remote: true, :class=>"icon-close" %></td>
</tr>